	<queryString>
		<![CDATA[select
        getdispl(ml.item) as typeofuse,
        sum(visits.visitors_count) as peopleserved,
        sum(visits.visitors_hours) as visithours,
        sum(staff.staff_hours) as staffhours
from uoc_common_methodlist ml
join uoc_common uc on (ml.id = uc.id)
join (select
                uc.id as uoc_id,
                sum(udg.usedatenumberofvisitors) as visitors_count,
                sum(udg.usedatenumberofvisitors * udg.usedatehoursspent) as visitors_hours
        from uoc_common uc
        join hierarchy hudg on (uc.id = hudg.parentid)
        join usedategroup udg on (hudg.id = udg.id)
        group by uc.id) visits on (uc.id = visits.uoc_id)
join (select
                uc.id as uoc_id,
                sum(sg.staffhours) as staff_hours
        from uoc_common uc
        join hierarchy hsg on (uc.id = hsg.parentid)
        join staffgroup sg on (hsg.id = sg.id)
        group by uc.id) staff on (uc.id = staff.uoc_id)
group by getdispl(ml.item)]]>
	</queryString>
